{
    "name": "joomla/joomla-cms",
    "type": "project",
    "description": "Joomla CMS",
    "keywords": ["joomla", "cms"],
    "homepage": "https://github.com/joomla/joomla-cms",
    "license": "GPL-2.0+",
    "config": {
        "platform": {
            "php": "5.5.9"
        },
        "vendor-dir": "libraries/vendor"
    },
    "support": {
        "issues":"https://issues.joomla.org",
        "irc":"http://irc.lc/freenode/joomla/",
        "forum":"http://forum.joomla.org/",
        "docs":"https://docs.joomla.org"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\": "libraries/src/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "joomla/application": "~1.5",
        "joomla/di": "~1.2",
        "joomla/event": "~2.0@dev",
        "joomla/registry": "~1.4 >=1.4.5",
        "joomla/session": "~2.0@dev",
        "joomla/string": "~1.3",
        "joomla/uri": "~1.1",
        "joomla/utilities": "~1.3 >=1.3.3",
        "ircmaxell/password-compat": "1.*",
        "leafo/lessphp": "0.5.0",
        "paragonie/random_compat": "~1.0",
        "phpmailer/phpmailer": "5.2.16",
        "symfony/polyfill-php55": "~1.2",
        "symfony/polyfill-php56": "~1.0",
        "symfony/yaml": "2.*",
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpunit/dbunit": "~1.3",
        "friendsofphp/php-cs-fixer": "~1.11",
        "squizlabs/php_codesniffer": "~1.5",
        "pear/cache_lite": "1.7.16"
    }
}
